

<h1>UnitE</h1>
<%= render 'search', q: @q, url: outfits_path %><br>
<hr>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>image</th>
      <th>いいね</th>
      <th>ユーザー名</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @outfits.each do |outfit| %>
       <tr id="outfit_<%= outfit.id %>">
        <%= render 'outfit', outfit: outfit %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<% if proposer_signed_in? %>
<%= link_to 'New Outfit', new_outfit_path %>
<% end %>